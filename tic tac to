    printf("player 2 won");
    goto end;
}
if(E1==1&&E5==1&&E9==1){
    printf("player 1 won");
    goto end;
}
if(E3==1&&E5==1&&E7==1){
    printf("player 1 won");
    goto end;
}
if(E1==2&&E5==2&&E9==2){
    printf("player 2 won");
    goto end;
}
if(E3==2&&E5==2&&E7==2){
    printf("player 2 won");
    goto end;
}
}
end:
}
    
}
